<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidaulaskotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppAulasKotlin">
        <activity android:name=".Paralelos.ActForResult.SecondScreen"></activity>
        <activity android:name=".Paralelos.ActForResult.FirstScreen" />
        <activity android:name="com.example.androidaulaskotlin.Paralelos.LoginMVP.LoginMVP" />
        <activity android:name="com.example.androidaulaskotlin.UdemyKotlin.estudos.MVVM.MVVM" />
        <activity android:name="com.example.androidaulaskotlin.UdemyKotlin.estudos.APIRetrofit.APIRetrofit" />
        <activity android:name=".ActsApps.Jokenpo.AppJokenpo" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>